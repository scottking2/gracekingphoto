---
import Layout from '../layouts/Layout.astro';
import Header from '../components/Header.astro';
import Footer from '../components/Footer.astro';

const pageTitle = "Newborn";
---

<Layout title={`${pageTitle} - Grace King Photography`}>
  <Header />
  <main class="pt-[92px]">
    <section class="min-h-screen py-20 px-8">
      <div class="max-w-7xl mx-auto">
        <h1 class="text-4xl md:text-5xl font-serif text-center mb-12">Newborn</h1>
        <p class="text-center text-lg">Content coming soon...</p>
      </div>
    </section>
  </main>
  <Footer />
</Layout>
